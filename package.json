    "postcss-nesting": "^2.1.1",
    "postcss-quantity-queries": "^0.4.0",
    "postcss-reporter": "^1.3.0",
    "react-addons-test-utils": "^0.14.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "sass-loader": "^3.0.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
